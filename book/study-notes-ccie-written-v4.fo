</fo:block><fo:list-block id="idm140619860882096" provisional-label-separation="0.2em" provisional-distance-between-starts="1.0em"><fo:list-item id="idm140619860881840" space-before.optimum="0.1em" space-before.minimum="0.1em" space-before.maximum="0.1em"><fo:list-item-label end-indent="label-end()"><fo:block>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block>
